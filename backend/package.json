    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.0.0",
    "nodemon": "^3.0.2",
    "prisma": "^6.18.0",
    "ts-node": "^10.9.2",
